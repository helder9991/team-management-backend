{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "ts-node-dev --respawn -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/server.ts",
    "build": "tsc && tsc-alias",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "seed:run": "RUN_SEED=true ts-node -r tsconfig-paths/register src/shared/database/typeorm/seeds/index.ts",
    "test": "jest --clearCache && NODE_ENV=test jest --coverage --runInBand  ",
    "queue": "ts-node-dev -r tsconfig-paths/register --inspect=9228 --transpile-only --ignore-watch node_modules ./src/shared/workers/bull/index.ts"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.22.7",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@jest/expect-utils": "^29.6.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/nodemailer": "^6.4.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.1",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.1",
    "redis": "^4.6.8",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.17",
    "yup": "^1.2.0"
